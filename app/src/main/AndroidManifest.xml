<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.salehe.kigambonibridge" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.android.alarm.permission.SET_AlARM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Home"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BridgeSituation" >
        </activity>
        <activity android:name=".TrafficNews" >
        </activity>
        <activity
            android:name=".PackagePayment"
            android:label="@string/title_activity_package_payment" >
        </activity>


        <receiver android:name="com.example.salehe.kigambonibridge.AlertReceiver" android:enabled="true"></receiver>
    </application>

</manifest>
